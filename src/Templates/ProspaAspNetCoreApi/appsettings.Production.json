{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "MetricsReportingHostedMetricsOptions": {
    "HostedMetrics": {
      "BaseUri": "(hostedMetricsBaseUri)",
      "ApiKey": "(hostedMetricsApiKeyProduction)"
    }
  },
  "SlackHealthAlertOptions": {
    "Channel": "#live-monitoring",
    "WebhookUrl": "(slackWebhookUrlProduction)"
  }
}